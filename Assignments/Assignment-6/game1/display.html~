$(document).ready(function() {	
	$( "#check").click(function() {
	 
	for (var i=0;i<f.words.length;i++)
	{
	var m='';
	var j=f.words[i].length;
	if(j==2)
	{var str=$("#a").text()+$("#b").text();}
	if(j==3)
	{var str=$("#a").text()+$("#b").text()+$("#c").text();}
	if(j==4)
	{var str=$("#a").text()+$("#b").text()+$("#c").text()+$("#d").text();}
	if(j==5)
	{var str=$("#a").text()+$("#b").text()+$("#c").text()+$("#d").text()+$("#e").text();}
	if(j==6)
	{var str=$("#a").text()+$("#b").text()+$("#c").text()+$("#d").text()+$("#e").text()+$("#f").text();}
	for(var n=0;n<j;n++)
	{
	m=m+f.words[i][n];
	}
	var g='';
	str=g+str;
	if(String(str)==String(m))
	{ $("#g").empty(); $("#g").append("Correct");break;}
	else{ $("#g").empty();$("#g").append("Try Again");}
	}
	});
	});
}
});
});
